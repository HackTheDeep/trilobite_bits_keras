              <div id="searchbox">
              <form action="http://www.uab.edu/search?" method="get">
                <input name="searchword" size="24" maxlength="128" alt="Search" value="Search UAB" onfocus="if(this.value=='Search UAB') this.value='';" id="q" onblur="if(this.value=='') this.value='Search UAB';" type="text">
                
                <input type="submit" >
              </form>
              </div>
            </li>
          </ul>
